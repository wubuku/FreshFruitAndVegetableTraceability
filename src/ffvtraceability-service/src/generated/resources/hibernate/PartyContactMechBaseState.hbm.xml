<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.dddml.ffvtraceability.domain.partycontactmech">
  <class name="org.dddml.ffvtraceability.domain.partycontactmech.AbstractPartyContactMechBaseState$SimplePartyContactMechBaseState" table="party_contact_mech_base">
    <composite-id name="partyContactMechBaseId" class="org.dddml.ffvtraceability.domain.partycontactmech.PartyContactMechBaseId">
      <key-property name="partyId">
        <column name="party_contact_mech_base_id_party_id" sql-type="VARCHAR(20)" />
      </key-property>
      <key-property name="contactMechId">
        <column name="party_contact_mech_base_id_contact_mech_id" sql-type="VARCHAR(20)" />
      </key-property>
    </composite-id>
    <version name="version" column="version" type="long" />
    <set name="protectedContactMechanisms" inverse="true" cascade="all-delete-orphan" fetch="join">
      <key>
        <column name="PARTY_ID" />
        <column name="CONTACT_MECH_ID" />
      </key>
      <one-to-many class="org.dddml.ffvtraceability.domain.partycontactmech.AbstractPartyContactMechState$SimplePartyContactMechState" />
    </set>
    <property name="activeFromDate">
      <column name="active_from_date" sql-type="timestamptz" />
    </property>
    <property name="createdBy">
      <column name="created_by" />
    </property>
    <property name="updatedBy">
      <column name="updated_by" />
    </property>
    <property name="createdAt" column="created_at" />
    <property name="updatedAt" column="updated_at" />
  </class>
</hibernate-mapping>
