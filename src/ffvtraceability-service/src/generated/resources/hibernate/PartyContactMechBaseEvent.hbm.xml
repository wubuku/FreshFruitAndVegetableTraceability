<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.dddml.ffvtraceability.domain.partycontactmech">
  <class name="AbstractPartyContactMechBaseEvent" table="party_contact_mech_base_event" abstract="true">
    <composite-id name="partyContactMechBaseEventId" class="org.dddml.ffvtraceability.domain.partycontactmech.PartyContactMechBaseEventId">
      <key-property name="partyContactMechBaseIdPartyId">
        <column name="party_contact_mech_base_id_party_id" sql-type="VARCHAR(20)" />
      </key-property>
      <key-property name="partyContactMechBaseIdContactMechId">
        <column name="party_contact_mech_base_id_contact_mech_id" sql-type="VARCHAR(20)" />
      </key-property>
      <key-property name="version">
        <column name="version" />
      </key-property>
    </composite-id>
    <discriminator column="event_type" type="string" />
    <property name="createdBy" column="created_by" />
    <property name="createdAt" column="created_at" />
    <property name="commandId" column="command_id" />
    <property name="commandType" column="command_type" length="50" />
    <subclass name="AbstractPartyContactMechBaseEvent$PartyContactMechBaseLobEvent" discriminator-value="PartyContactMechBaseLobEvent">
      <property name="dynamicProperties">
        <column name="dynamic_properties" sql-type="json" />
        <type name="io.hypersistence.utils.hibernate.type.json.JsonType">
          <param name="org.hibernate.type.ParameterType">java.util.HashMap</param>
        </type>
      </property>
    </subclass>
    <subclass name="AbstractPartyContactMechBaseEvent$AbstractPartyContactMechBaseStateEvent" abstract="true">
      <property name="activeFromDate">
        <column name="active_from_date" sql-type="timestamptz" />
      </property>
      <subclass name="AbstractPartyContactMechBaseEvent$SimplePartyContactMechBaseStateCreated" discriminator-value="Created"></subclass>
      <subclass name="AbstractPartyContactMechBaseEvent$SimplePartyContactMechBaseStateMergePatched" discriminator-value="MergePatched">
        <property name="isPropertyActiveFromDateRemoved" column="is_property_active_from_date_removed" />
      </subclass>
    </subclass>
  </class>
</hibernate-mapping>
