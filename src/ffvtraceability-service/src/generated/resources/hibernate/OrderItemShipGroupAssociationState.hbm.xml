<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"[]>
<hibernate-mapping package="org.dddml.ffvtraceability.domain.order">
  <class name="org.dddml.ffvtraceability.domain.order.AbstractOrderItemShipGroupAssociationState$SimpleOrderItemShipGroupAssociationState" table="order_item_ship_group_association">
    <composite-id name="orderItemShipGroupAssociationId" class="org.dddml.ffvtraceability.domain.order.OrderItemShipGroupAssociationId">
      <key-property name="orderId">
        <column name="ORDER_ID" sql-type="VARCHAR(20)" />
      </key-property>
      <key-property name="orderShipGroupShipGroupSeqId">
        <column name="SHIP_GROUP_SEQ_ID" sql-type="VARCHAR(20)" />
      </key-property>
      <key-property name="orderItemSeqId">
        <column name="ORDER_ITEM_SEQ_ID" sql-type="VARCHAR(20)" />
      </key-property>
    </composite-id>
    <version name="version" column="version" type="long" />
    <many-to-one name="protectedOrderShipGroupState" class="org.dddml.ffvtraceability.domain.order.AbstractOrderShipGroupState$SimpleOrderShipGroupState" insert="false" update="false">
      <column name="ORDER_ID" />
      <column name="SHIP_GROUP_SEQ_ID" />
    </many-to-one>
    <property name="quantity">
      <column name="quantity" sql-type="DECIMAL(18,6)" />
    </property>
    <property name="cancelQuantity">
      <column name="cancel_quantity" sql-type="DECIMAL(18,6)" />
    </property>
    <property name="createdBy">
      <column name="created_by" />
    </property>
    <property name="updatedBy">
      <column name="updated_by" />
    </property>
    <property name="deleted">
      <column name="deleted" />
    </property>
    <property name="createdAt" column="created_at" />
    <property name="updatedAt" column="updated_at" />
  </class>
</hibernate-mapping>
